	font-family: 'NeutraTextTFBookAlt';
	font-size: 17px;
	color: #333;
	text-transform: none;
	padding-bottom: 10px;
}
.sidebar #negocie{
	width: 190px;
	border-top: 1px solid #DEDCD5;
    margin-top: 40px;
    padding-top: 30px;
}
.sidebar #newsletter{
	width: 190px;
	border-top: 1px solid #DEDCD5;
    margin-top: 40px;
    padding-top: 30px;
    border-left: 0;
    border-right: 0;
    padding-left: 0;
    padding-right: 0;
    margin-left: 0;
    margin-right: 0;
}

.nav-investidores{

	padding-top:36px;

	ul{
		
	}
	li{
		list-style-type:none !important;
		border-top:1px solid #e2e2e2;
		padding:35px 0 !important;

		&:first-child{
			border-top:none;
		}
		b{
			font-weight: bold;
		}
	}
	a{
		color:#056839;
		font-family: 'NeutraTextTFBookAlt';
		font-size:30px;
		text-transform: uppercase;
		text-decoration: none;
		widows: 710px;
		padding-left: 26px;
		display: block;
		background: url("../img/investidores.png") no-repeat scroll 0 -80px transparent;
	}
}
.investidores-content{
	padding-top:30px;
	color:#666;
	display:none;
}
.image-porque-investir{
	margin: auto;
    width: 376px;
}
.image-sete-lagoas{
	padding-top:20px;
}
.image-grancorp{
	padding-top:20px;
}
.image-tabela{
	padding-top:20px;
}

.form-padrao{

	width:550px;

	input[type="text"], input[type="email"]{
		width: 532px;
		height: 35px;
		border:1px solid #dedcd5;
		background-color:#f0f0ed;
		border-radius: 3px;
		margin: 10px 0;
		padding-left:18px;
		font-family: 'NeutraTextTFDemiAlt';
		font-size:12px;
		color:#999;
		text-transform: uppercase;
	}

	input[type="text"].mini, input[type="email"].mini{
		width: 330px;
	}

	textarea{
		width: 516px;
		border:1px solid #dedcd5;
		background-color:#f0f0ed;
		border-radius: 3px;
		margin: 10px 0;
		height: 132px;
		font-family: 'NeutraTextTFDemiAlt';
		font-size:12px;
		color:#999;
		text-transform: uppercase;
		padding:17px;
		resize: none;
	}
	input[type="submit"]{
	float: right;
	color:#FFF;
	text-decoration: none;
	font-size: 18px;
	text-transform: uppercase;
	width: 149px;
	padding-top: 3px;
	height: 35px;
	margin-top:20px;
	border: 1px solid #a0a0a0;
	border-radius: 5px;
	text-align: center;
	font-family: 'NeutraTextTFDemiAlt';
	background: #044c2a;
	background: -moz-linear-gradient(top, #068348 0%, #044c2a 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#068348), color-stop(100%,#044c2a));
	background: -webkit-linear-gradient(top, #068348 0%,#044c2a 100%);
	background: -o-linear-gradient(top, #068348 0%,#044c2a 100%);
	background: -ms-linear-gradient(top, #068348 0%,#044c2a 100%);
	background: linear-gradient(to bottom, #068348 0%,#044c2a 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#068348', endColorstr='#044c2a',GradientType=0 );
	}

	select{
		height: 35px;
		padding-top:7px;
		direction: block;
	}
}


.vaga-item{
	padding-bottom:100px;
	width:710px;
	padding-top:50px;
	border-bottom:1px solid #dedede;

	p{
		width: 510px;
	}
}






