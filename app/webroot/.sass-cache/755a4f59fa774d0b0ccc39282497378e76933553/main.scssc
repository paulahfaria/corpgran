$headerBgColor:#f0f0ed;
$footerBgColor:#076e3d;
$subFooterBgColor:#02512b;
$bodyBgColor:#FFF;

$baseTextColor: #666;
$subTextColor: #FFF;
$highlightColor:#056839;
$subHighlightColor:#333;

$projectHighlightBgColor:#056839;

@mixin containerCenter(){
	width: 950px;
	margin:auto;
	padding: 0 5px;
}

@font-face {
    font-family: 'NeutraTextTFDemiAlt';
    src: url('fonts/neutratexttf-demialt.eot');
    src: url('fonts/neutratexttf-demialt.eot') format('embedded-opentype'),
         url('fonts/neutratexttf-demialt.woff') format('woff'),
         url('fonts/neutratexttf-demialt.ttf') format('truetype'),
         url('fonts/neutratexttf-demialt.svg#NeutraTextTFDemiAlt') format('svg');
}

@font-face {
    font-family: 'NeutraTextTFBookAlt';
    src: url('fonts/neutratexttf-bookalt.eot');
    src: url('fonts/neutratexttf-bookalt.eot') format('embedded-opentype'),
         url('fonts/neutratexttf-bookalt.woff') format('woff'),
         url('fonts/neutratexttf-bookalt.ttf') format('truetype'),
         url('fonts/neutratexttf-bookalt.svg#NeutraTextTFBookAlt') format('svg');
}


body{
	font-size: 12px;
	font-family: Arial;
	color: $baseTextColor;
}

header{
	font-family: 'NeutraTextTFDemiAlt';
	border-bottom: 3px solid #006445;
	width: 100%;
	height: 220px;
	position: relative;
	background: #fefefe;
	background: -moz-linear-gradient(top, #fefefe 0%, #ddddda 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fefefe), color-stop(100%,#ddddda));
	background: -webkit-linear-gradient(top, #fefefe 0%,#ddddda 100%);
	background: -o-linear-gradient(top, #fefefe 0%,#ddddda 100%);
	background: -ms-linear-gradient(top, #fefefe 0%,#ddddda 100%);
	background: linear-gradient(to bottom, #fefefe 0%,#ddddda 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#ddddda',GradientType=0 );
}
header:before{
	bottom: -3px;
    box-shadow: 0 -3px 5px #000000;
    content: "";
    height: 1px;
    position: absolute;
    width: 100%;
}
#header-container{
	@include containerCenter();
}
#home-content{
	@include containerCenter();
	overflow: hidden;
}

.logo{
	margin-top: 43px;
	float: left;
}
#social-nav{
	float: right;
	text-transform: uppercase;

	li{
		float: left;
	}

	a{
		background-color: #dbdbdb;
		display: block;
		color: $baseTextColor;
		text-decoration: none;
		font-size: 10px;
		text-align: center;
	}

	.facebook, .email{
		background: url("../img/icons.png") no-repeat scroll 0 0 #dbdbdb;
		border-radius: 20px;
		width: 31px;
		text-indent: -9999px;
		height: 31px;
		margin: 7px 5px 0;
	}
	.facebook{
		background-position: 13px 9px;
	}
	.email{
		background-position: -28px 9px;
	}
	.acompanhar-empreendimento, .login{
		padding-top: 12px;
		height: 25px;
	}
	.acompanhar-empreendimento{
		width: 206px;
		margin: 0 1px 0 20px;
	}
	.login{
		width: 63px;
	}
}
.header-telephone{
	color: $highlightColor;
	font-size: 25px;
    float: right;
    margin: 56px 0 0 0;
    text-transform: uppercase;
}

.main-nav{
	float: left;
    height: 67px;
    width: 960px;
    border-top: 1px solid #FFF;
    margin-top: 36px;
    position: relative;

    li{
    	float: left;
    	margin-left: 41px;
    }
    li:first-child{
    	margin-left: 5px;
    }
    a{
    	font-size: 13px;
    	text: {
    		transform: uppercase;
    		decoration: none;
    	}
    	color: $baseTextColor;
    	padding: 25px 0;
    	display: block;
    }
}
.main-nav:before{
	border-top: 1px solid #CAC9C5;
    content: "";
    position: absolute;
    top: -2px;
    width: 960px;
}

#banner{
	background: url("../img/banner/1.jpg") no-repeat scroll center 0 transparent;
    height: 327px;
    width: 100%;
}

.main{
	min-height: 800px;
}

.list-obras{
	width: 720px;
	padding-top: 65px;
	float: left;
}

.filter-nav{
